<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusID">ID</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" type="number" id="cusId" name="cusId" value="<%= model.cusId %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusFname">First Name</label>
    <div class="col-sm-7">
      <input class="form-control"  type="text" id="cusFname" name="cusFname" value="<%= model.cusFname %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusLname">Last Name</label>
    <div class="col-sm-7">
      <input class="form-control"  type="text" id="cusLname" name="cusLname" value="<%= model.cusLname %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusState">State</label>
    <div class="col-sm-7">
        <input class="form-control" type="text" id="cusState" name="cusState" value="<%= model.cusState %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusSalesYTD">Sales YTD</label>
        <div class="col-sm-7">
            <input class="form-control" type="text" id="cusSalesYTD" name="cusSalesYTD" value="<%= model.cusSalesYTD %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusSalesPrev">Sales Preivous Years</label>
        <div class="col-sm-7">
            <input class="form-control" type="text" id="cusSalesPrev" name="cusSalesPrev" value="<%= model.cusSalesPrev %>" />
    </div>
  </div>